{
  "expo": {
    "name": "YTAT",
    "slug": "YTAT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconY.png",
    "plugins": [
      [
        "expo-notifications"
      ]
    ],
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./assets/splashY.png",
        "resizeMode": "contain",
        "backgroundColor": "black"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconY.png",
        "backgroundColor": "black"
      },
      "package": "com.levdm.Diary"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
