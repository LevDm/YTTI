{
  "expo": {
    "name": "YTAT",
    "slug": "YTAT",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-notifications"
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "inset-swipe",
          "backgroundColor": "#00000080"
        }
      ]
    ],
    "extra": {
      "WEATHER_API_KEY": "api_key"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "userInterfaceStyle": "automatic",
      "icon": "./assets/iconY.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground_iconY.png",
        "backgroundColor": "#000000",
        "backgroundImage": "./assets/background_iconY.png",
        "monochromeImage": "./assets/monochrome_iconY.png"
      },
      "splash": {
        "resizeMode": "native",
        "image": "./assets/splashY.png",
        "backgroundColor": "#000000"
      },
      "package": "com.F1F.YTAT",
      "jsEngine": "hermes"
    }
  }
}
